{
  "name": "procart",
  "version": "1.0.0",
  "description": "e-commerce app built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js -i",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Srikar-D-Palmite/pro-cart.git"
  },
  "author": "Srikar Tadeparti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Srikar-D-Palmite/pro-cart/issues"
  },
  "homepage": "https://github.com/Srikar-D-Palmite/pro-cart#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0"
  }
}
